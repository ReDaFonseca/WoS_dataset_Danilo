@article{ WOS:000814465500004,
Author = {El-kenawy, El-Sayed M. and Zerouali, Bilel and Bailek, Nadjem and
   Bouchouich, Kada and Hassan, Muhammed A. and Almorox, Javier and Kuriqi,
   Alban and Eid, Marwa and Ibrahim, Abdelhameed},
Title = {Improved weighted ensemble learning for predicting the daily reference
   evapotranspiration under the semi-arid climate conditions},
