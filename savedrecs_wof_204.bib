@article{ WOS:000533277500006,
Author = {Neary, Vincent S. and Ahn, Seongho and Seng, Bibiana E. and Allahdadi,
   Mohammad Nabi and Wang, Taiping and Yang, Zhaoqing and He, Ruoying},
Title = {Characterization of Extreme Wave Conditions for Wave Energy Converter
   Design and Project Risk Assessment},
