@article{ WOS:001171521800004,
Author = {Shweta, R. and Sivagnanam, S. and Kumar, K. A.},
Title = {IoT-based Deep Learning Neural Network (DLNN) algorithm for voltage
   stability control and monitoring of solar power generation},
