@article{ WOS:001021339400001,
Author = {Kirby, Andrew and Briol, Francois-Xavier and Dunstan, Thomas D. and
   Nishino, Takafumi},
Title = {Data-driven modelling of turbine wake interactions and flow resistance
   in large wind farms},
