@article{ WOS:001029358500001,
Author = {Vinothkumar, T. and Deepa, S. N. and Raj, F. Vijay Amirtha},
Title = {Adaptive probabilistic neural network based on hybrid PSO-ALO for
   predicting wind speed in different regions},
