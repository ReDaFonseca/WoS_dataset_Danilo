@article{ WOS:000900854900001,
Author = {Oh, JeongRim and Park, JongJin and Ok, ChangSoo and Ha, ChungHun and
   Jun, Hong-Bae},
Title = {A Study on the Wind Power Forecasting Model Using Transfer Learning
