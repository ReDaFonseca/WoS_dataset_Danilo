@article{ WOS:001061019700001,
Author = {Islam, M. K. and Hassan, N. M. S. and Rasul, M. G. and Emami, Kianoush
   and Chowdhury, Ashfaque Ahmed},
Title = {Forecasting of Solar and Wind Resources for Power Generation},
Journal = {ENERGIES},
Year = {2023},
Volume = {16},
Number = {17},
Month = {SEP},
Abstract = {Solar and wind are now the fastest-growing power generation resources,
   being ecologically benign and economical. Solar and wind forecasts are
   significantly noteworthy for their accurate evaluation of renewable
   power generation and, eventually, their ability to provide profit to the
   power generation industry, power grid system and local customers. The
   present study has proposed a Prophet-model-based method to predict solar
   and wind resources in the Doomadgee area of Far North Queensland (FNQ),
   Australia. A SARIMA modelling approach is also implemented and compared
   with Prophet. The Prophet model produces comparatively less errors than
   SARIMA such as a root mean squared error (RMSE) of 0.284 and a mean
   absolute error (MAE) of 0.394 for solar, as well as a MAE of 0.427 and a
   RMSE of 0.527 for wind. So, it can be concluded that the Prophet model
   is efficient in terms of its better prediction and better fitting in
   comparison to SARIMA. In addition, the present study depicts how the
   selected region can meet energy demands using their local renewable
   resources, something that can potentially replace the present dirty and
   costly diesel power generation of the region.},
