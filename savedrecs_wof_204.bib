@article{ WOS:000907857700002,
Author = {Oyando, Harold Chisano and Kanyolo, Timothy Ngumbi and Chang, Choong-koo},
Title = {RNN-Based Main Transformer OLTC Control for SMR Integration into a High
   Renewable Energy Penetrated Grid},
Journal = {JOURNAL OF ELECTRICAL ENGINEERING \& TECHNOLOGY},
Year = {2023},
