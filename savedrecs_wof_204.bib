@article{ WOS:001028323700001,
Author = {Hissou, Hasna and Benkirane, Said and Guezzaz, Azidine and Azrour,
   Mourade and Beni-Hssane, Abderrahim},
Title = {A Novel Machine Learning Approach for Solar Radiation Estimation},
Journal = {SUSTAINABILITY},
Year = {2023},
